<?xml version="1.0"?>
<module name="process">
  <class type="req" syntax="(*args, [working_dir])"
         name="ExecuteAction" message="ProcessExecuteAction">
    <description>process execute action => process execute reply</description>
  </class>
  <class type="rep" syntax="(pid)"
         name="ExecuteReply" message="ProcessExecuteReply">
    <description>process execute reply</description>
  </class>
  <class type="req" syntax="(pid, fileno, chunk)"
         name="StreamAction" message="ProcessStreamMessage">
    <description>stream data action => ack</description>
  </class>
  <class type="upd" syntax="(pid, fileno, chunk)"
         name="StreamUpdate" message="ProcessStreamMessage">
    <description>stream data update</description>
  </class>
  <class type="req" syntax="(pid, signal)"
         name="SignalAction" message="ProcessSignalAction">
    <description>process signal action => ack</description>
  </class>
  <class type="upd" syntax="(pid, exit_code)"
         name="ExitUpdate" message="ProcessExitUpdate">
    <description>process exit update</description>
  </class>
</module>
